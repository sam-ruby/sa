:javascript
  var Selected_Date = new Date(
    #{@date.year}, #{@date.month-1}, #{@date.day});
  var Available_weeks = #{@available_weeks.to_json} 
  var Min_date = new Date(
    #{@min_date.year}, #{@min_date.month - 1}, #{@min_date.day});
  var Max_date = new Date(
    #{@max_date.year}, #{@max_date.month - 1}, #{@max_date.day});

#dashboard.row-fluid
  .span6
    #dashboard-search-quality-container
      .content-header
        Search Quality Queries
        %button.close.pull-right
          &times;
      .content
        = image_tag 'ajax_loader.gif', :class=>'ajax-loader'
        
  .span6
    #dashboard-poorly-performing-container
      .content-header
        Poorly Performing Product Intents
        %button.close.pull-right
          &times;
      .content
        = image_tag 'ajax_loader.gif', :class=>'ajax-loader'

#search-quality
  #search-quality-queries
    #search-quality-filter
    = image_tag 'ajax_loader.gif', :class=>'ajax-loader'
   
#trending{:style=>'display:none'}
  %ul.nav.nav-pills.trending
    %li.active
      %a.pp{:href=>"#"}
        Poor performing within last 30 days
    %li
      %a.up{:href=>"#"}
        Trending Queries

    %li.period-selector{:style=>'display:none'}
      %span
        &nbsp;
      %label.radio.inline
        %input.two-days{:type => :radio, :value => '2d', 
          :name=>:trending_radio, :checked => true}
        last 2 days
      
      %label.radio.inline
        %input.one-week{:type => :radio, :value => '1w', :name=>:trending_radio}
        last 1 week

      %label.radio.inline
        %input.two-week{:type => :radio, :value => '2w', :name=>:trending_radio}
        last 2 weeks

      %label.radio.inline
        %input.three-week{:type => :radio, :value => '3w', :name=>:trending_radio}
        last 3 weeks
      
      %label.radio.inline
        %input.four-week{:type => :radio, :value => '3w', :name=>:trending_radio}
        last 4 weeks


  .pp-content
    = image_tag 'ajax_loader.gif', :class=>'ajax-loader'
  .up-content
    = image_tag 'ajax_loader.gif', :class=>'ajax-loader'

