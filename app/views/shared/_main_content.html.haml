#summary-metric-car.carousel.slide{'data-interval' => 'false'}
  .carousel-inner{style: 'height: 900px;'}
    #overall-metrics.active.item
      = image_tag 'ajax_loader.gif', :class=>'ajax-loader'
    #summary-metrics.item
      = image_tag 'ajax_loader.gif', :class=>'ajax-loader'
    .item{style: 'height: 900px'}
      #metric
        .tab-holder
          = image_tag 'ajax_loader.gif', :class=>'ajax-loader'
        #metric-car.carousel.slide{'data-interval' => 'false',
          style: 'height:400px'}
          .carousel-inner
            .distribution.hcharts.active.item
            .item.timeline

          %a.carousel-control.left{href: '#metric-car',
            'data-slide' => 'prev'}
            &lsaquo;
          %a.carousel-control.right{href: '#metric-car',
            'data-slide' => 'next'}
            &rsaquo;
      
      #winners.winners{style: 'height:20em'}
        = image_tag 'ajax_loader.gif', :class=>'ajax-loader'

    #search-view.item.details{style: 'height: 1100px'}
